    "Crunchyroll",
    "Party"
  ],
  "author": "SamuraiExx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samuraiexx/roll_together_backend/issues"
  },
  "homepage": "https://github.com/samuraiexx/roll_together_backend#readme",
  "dependencies": {
    "express": "^4.17.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "socket.io": "^4.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "nodemon": "^2.0.4"
  },
  "optionalDependencies": {
    "fsevents": "2.1.2"
  }
